{
   "name": "backend",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "type": "commonjs",
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "migration:create": "typeorm  migration:create -d src/infrastructure/database/migrations/ -n",
      "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/infrastructure/config/typeorm/typeOrm.config.ts",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --debug --watch",
      "start:debug": "ts-node-dev src/main.ts",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "ts-node ./node_modules/typeorm/cli"
   },
   "dependencies": {
      "@fastify/static": "^6.10.2",
      "@nestjs/common": "^9.0.0",
      "@nestjs/core": "^9.0.0",
      "@nestjs/platform-express": "^9.0.0",
      "@nestjs/platform-fastify": "^9.4.2",
      "@nestjs/swagger": "^7.1.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.2.0",
      "typeorm": "0.3.16"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.0.0",
      "@nestjs/config": "^2.3.2",
      "@nestjs/schematics": "^9.0.0",
      "@nestjs/testing": "^9.0.0",
      "@nestjs/typeorm": "^9.0.1",
      "@types/express": "^4.17.13",
      "@types/jest": "29.5.1",
      "@types/node": "^16.18.34",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "dotenv": "^16.1.3",
      "editorconfig": "^1.0.2",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "29.5.0",
      "pg": "^8.11.0",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "29.1.0",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "4.2.0",
      "typeorm": "^0.3.16",
      "typescript": "4.5.2",
      "webpack": "^5.85.0",
      "webpack-cli": "^5.1.1"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
